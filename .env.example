# Branding
NEXT_PUBLIC_APP_NAME=SAGES Chat

# NextAuth
NEXTAUTH_URL=https://escuelasabatica.sages.icu
NEXTAUTH_SECRET=your-nextauth-secret-here

# Google OAuth
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret

# n8n
N8N_BASE_URL=https://sswebhookss.sages.icu
N8N_WEBHOOK_PATH=/webhook/your-webhook-path
N8N_API_KEY=your-n8n-api-key-here

# Python RAG Backend
PYTHON_BACKEND_URL=http://backend:5000

# i18n
DEFAULT_LOCALE=es-ES
SUPPORTED_LOCALES=es-ES,en-US,es-CO

# Database
DATABASE_URL=postgresql://user:password@host:5432/database

# Gemini API
GEMINI_API_KEY=your-gemini-api-key-here

# Qdrant Vector Database
QDRANT_URL=https://your-qdrant-instance.com
QDRANT_API_KEY=your-qdrant-api-key-here
QDRANT_COLLECTION=your-collection-name

# Environment
NODE_ENV=production
LOG_LEVEL=info
APP_VERSION=1.0.0
